class Solution {
    public boolean hasSameDigits(String s) {
        while(s.length()!=2){
            String res="";

            for(int i=0;i<s.length()-1;i++){
                char ch=(char)((s.charAt(i)-'0'+s.charAt(i+1)-'0')%10+'0');
                res+=ch;
            }
            s=res;
        }

        return s.charAt(0)==s.charAt(1)?true:false;
    }
}
