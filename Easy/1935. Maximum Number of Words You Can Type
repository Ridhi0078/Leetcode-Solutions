class Solution {
    public int canBeTypedWords(String text, String brokenLetters) {
        String str[] = text.split(" ");
        int n = str.length;
        int count = 0;
        for (int i = 0; i < str.length; i++) {
            String s1 = str[i];

            for (int j = 0; j < s1.length(); j++) {
                char ch = s1.charAt(j);

                if (brokenLetters.indexOf(ch) != -1) {
                    count++;
                    break;
                }
            }
        }
        return n - count;
    }
}
