class Solution {
    public int[] getNoZeroIntegers(int n) {
        int arr[] = new int[2];

        for (int i = 0; i < n; i++) {
            int a = i;
            int b = n - i;
            if (findZero(a) && findZero(b)) {
                arr[0] = a;
                arr[1] = b;
            }
        }
        return arr;
    }

    public boolean findZero(int n) {
        while (n > 0) {
            int rem = n % 10;

            if (rem == 0) {
                return false;
            }
            n = n / 10;
        }
        return true;
    }
}
