class Solution {
    public int numberOfBeams(String[] bank) {
        List<Integer> ans = new ArrayList<>();
        for (int i = 0; i < bank.length; i++) {
            String s1 = bank[i];
            int count = 0;
            for (char ch : s1.toCharArray()) {
                if (ch == '1') {
                    count++;
                }
            }
            if (count != 0) {
                ans.add(count);
            }
        }

        int res = 0;
        for (int i = 0; i < ans.size() - 1; i++) {
            res += ans.get(i) * ans.get(i + 1);
        }
        return res;
    }
}
